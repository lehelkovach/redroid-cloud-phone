[Unit]
Description=SSH Tunnel for Waydroid VNC and API
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User=%i
ExecStart=/usr/bin/ssh -i %h/.ssh/waydroid_oci -o StrictHostKeyChecking=no -o ServerAliveInterval=60 -o ServerAliveCountMax=3 -o ExitOnForwardFailure=yes -N -L 5901:localhost:5901 -L 8080:localhost:8080 ubuntu@161.153.55.58
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=default.target

