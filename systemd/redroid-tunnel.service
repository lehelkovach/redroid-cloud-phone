[Unit]
Description=SSH Tunnel for Redroid VNC and API
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User=%i
ExecStart=/usr/bin/ssh -i %h/.ssh/redroid_oci -o StrictHostKeyChecking=no -o ServerAliveInterval=60 -o ServerAliveCountMax=3 -o ExitOnForwardFailure=yes -N -L 5900:localhost:5900 -L 8080:localhost:8080 ubuntu@137.131.52.69
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=default.target
